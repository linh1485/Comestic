@model IEnumerable<demo2_mp.Models.Category>

<link href="~/Content/css/user/danhmuc.css" rel="stylesheet" />
<a href="~/Products/ProductList" >
    <h3 class="category-heading">
        <i class="fa-solid fa-bars"></i>
        Danh mục
    </h3>
</a>

<ul class="category-list">
    @{
        foreach (var item in Model)
        {
            <li class="category-item-link">
                @Html.ActionLink(item.NameCate, "ProductList", "Products", new { category = item.IDCate }, "")
            </li>
        }
    }
</ul>


<style>
    a {
        text-decoration: none;
        color: inherit;
    }
</style>
